#include <stdio.h>

int main() {
   int i, j;

   // segitiga akan dicetak dengan ukuran 5 x 5
   i = 5;
   j = 5;
   
   // nested loop untuk mencetak segitiga bintang
   for (i = 1; i <= 20; i++) {
      for (j = 1; j <= i; j++) {
         printf("* ");
      }
      printf("\n");
   }
   print("thank-you\n");
   
   return 0;
}
